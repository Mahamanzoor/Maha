
public class mylinklist extends list {

	private nodes list;
	private nodes First = null;
	private nodes Last = null;
	
	public void add(int num){
		
		list = new nodes();
		list.num=num;
		list.self=list;
		list.next=null;
		list.previous=null;
		
		if(First == Last && Last != null)
		{
			First.next = list;
		}
		
		
		if(First == null)
		{
			First = list;
		}
		
		if(Last == null)
		{
			Last = list;
		}
		
		
		
	}
